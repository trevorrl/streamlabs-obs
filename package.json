    "vue-popperjs": "1.2.2",
    "vue-property-decorator": "7.0.0",
    "vue-slider-component": "2.2.7",
    "vue-template-compiler": "2.5.16",
    "vue-toasted": "^1.1.24",
    "vuedraggable": "2.8.5",
    "vuex": "3.0.1",
    "webpack": "4.8.3",
    "webpack-cli": "2.1.3"
  },
  "resolutions": {
    "uglify-es": "3.3.9",
    "spectron/**/electron-chromedriver": "3.0.0-beta.1"
  }
}